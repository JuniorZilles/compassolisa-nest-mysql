{
  "moduleFileExtensions": ["js", "json", "ts"],
  "bail": true,
  "clearMocks": true,
  "coveragePathIgnorePatterns": ["/node_modules/", "/src/main.ts"],
  "testPathIgnorePatterns": ["/node_modules/", "/utils/", "/coverage/", "/setup.features.ts"],
  "transformIgnorePatterns": ["/node_modules/", "/coverage/"],
  "collectCoverageFrom": ["src/**"],
  "collectCoverage": false,
  "coverageDirectory": "coverage",
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "preset": "ts-jest",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testMatch": [
  "**/tests/**/*.spec.ts?(x)"
  ]
}
